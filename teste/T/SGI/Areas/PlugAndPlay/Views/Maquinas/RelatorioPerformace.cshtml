
@{
    ViewBag.Title = "Performace";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section scripts{
    <script src="http://code.highcharts.com/stock/highstock.js"></script>
    <script src="http://code.highcharts.com/stock/modules/exporting.js"></script>
    <script src="~/Areas/PlugAndPlay/Scripts/Maquinas/ConfigEventos.js"></script>
    <script src="~/Areas/PlugAndPlay/Scripts/Maquinas/CtxGraficos.js"></script>
}
<div class='col-xs-12'>
    <div class="page-title">
        <div class="pull-left">
            <h1 class="title">@ViewData["Title"]</h1>
        </div>

        <div class="pull-right hidden-xs">
            <ol class="breadcrumb">
                <li>
                    <a href="@Url.Action("Index","Home")"><i class="fa fa-home"></i>Home</a>
                </li>
                <li>
                    <i class="fa fa-tags"></i>Cadastros
                </li>
                <li>
                    <a href="@Url.Action("Index","Maquinas")">Maquinas</a>
                </li>
                <li class="active">
                    <strong>Maquina</strong>
                </li>
            </ol>
        </div>
    </div>
</div>
<div class="col-xs-12">
    <section class="box">
        <header>
            <h2>
                Performance da Máquinas
            </h2>
        </header>
        <div id="divGraficos" class="content-body">
            
        </div>
    </section>
</div>

